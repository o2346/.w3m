#Page/Cursor motion
keymap C-e UP 4
keymap C-y DOWN 4
keymap C-d UP 24
keymap C-u DOWN 24
keymap C-f NEXT_PAGE
keymap C-b NULL
keymap C-b PREV_PAGE

keymap C-n NULL
keymap C-p NULL
keymap j NULL
keymap k NULL
keymap g-g BEGIN
keymap j MOVE_DOWN1
keymap k MOVE_UP1
keymap ";"-G GOTO_LINE
keymap 0 LINE_BEGIN

#Hyperlink operation
keymap o NULL
#keymap o GOTO_LINK
keymap "'" TAB_LINK
keymap a PEEK
keymap C-a PEEK_LINK
keymap A PEEK_IMG
keymap ";"-e EXTERN
keymap ";"-E EXTERN_LINK

#File/Stream operation

#Buffer operation
keymap L NULL
keymap H NULL
keymap L NEXT
keymap H PREV
keymap ";"-h HISTORY
keymap r RELOAD
keymap R RESHAPE
keymap C-r REDRAW

keymap i DISPLAY_IMAGE

#Tab operation
#keymap C-j NULL
#C-jâ”‚Go to current link (GOTO_LINK) <- Why return key disabled when this is absent?
keymap C-m NULL
keymap C-t NULL
keymap ] NULL
keymap [ NULL
keymap ] NEXT_TAB
keymap [ PREV_TAB
keymap C-h NULL
keymap C-l NULL
keymap C-h TAB_LEFT
keymap C-l TAB_RIGHT
keymap U NULL
keymap o GOTO
keymap t TAB_GOTO
keymap d CLOSE_TAB
#keymap T TAB_MENU

#Bookmark operation

#Search
keymap C-s NULL
keymap / ISEARCH
keymap ? ISEARCH_BACK

#Dictionary look-up

#Mark operation
keymap v MARK_WORD

#Miscellany
keymap ";"-s VIEW
keymap ":"-! EXEC_SHELL
keymap M NULL
keymap ESC-M NULL
keymap ";"-e EXTERN
keymap ";"-E EXTERN_LINK
keymap c COMMAND
keymap ";"-m MOVE_LIST_MENU
keymap ";"-? HELP
keymap ";"-p OPTIONS
keymap D NULL
keymap ";"-d DOWNLOAD_LIST
keymap C-C NULL
keymap ";"-k COOKIE
keymap ~ REINIT

#User defined keymaps
keymap x NULL
keymap y NULL
keymap x EXTERN "[ $(uname) = \"Darwin\" ] && echo %s | pbcopy || echo %s | xsel --clipboard --input"
keymap y SHELL "[ $(uname) = \"Darwin\" ] && echo $W3M_CURRENT_LINK | pbcopy || echo $W3M_CURRENT_LINK | xsel --clipboard --input"

#generate markdown link
#$W3M_TITLE mojibake happens when it comes with japanese. try cgi?
#keymap ";"-L SHELL "echo [$W3M_TITLE]\($W3M_URL\) | xsel --clipboard --input"
keymap ";"-u NULL
keymap ";"-u SHELL "[ $(uname) = \"Darwin\" ] && echo []\($W3M_URL\) | pbcopy || echo []\($W3M_URL\) | xsel --clipboard --input"
keymap ";"-U NULL
keymap ";"-U SHELL "[ $(uname) = \"Darwin\" ] && echo []\($W3M_CURRENT_LINK\) | pbcopy || echo []\($W3M_CURRENT_LINK\) | xsel --clipboard --input"
keymap V NULL

