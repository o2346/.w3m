#Page/Cursor motion
keymap C-e UP
keymap C-y DOWN
keymap C-f NEXT_PAGE
keymap C-b NULL
keymap C-b PREV_PAGE

keymap C-n NULL
keymap C-p NULL
keymap j NULL
keymap k NULL
keymap g-g BEGIN
keymap j MOVE_DOWN1
keymap k MOVE_UP1
keymap ";"-G GOTO_LINE
keymap 0 LINE_BEGIN

#Hyperlink operation
keymap o NULL
keymap o GOTO_LINK
keymap t TAB_LINK
keymap a PEEK
keymap C-a PEEK_LINK
keymap A PEEK_IMG
keymap ";"-e EXTERN
keymap ";"-l EXTERN_LINK
#File/Stream operation
#Buffer operation
keymap L NEXT
keymap H PREV
keymap ";"-h HISTORY
keymap r RELOAD
keymap R RESHAPE
keymap C-r REDRAW

keymap i DISPLAY_IMAGE
#Tab operation
keymap C-l NULL
keymap C-h NULL
#keymap C-j NULL
#C-jâ”‚Go to current link (GOTO_LINK) <- Why return key disabled when this is absent?
keymap C-m NULL
keymap C-t NULL

keymap C-l NEXT_TAB
keymap C-h PREV_TAB
keymap { NULL
keymap } NULL
keymap } TAB_RIGHT
keymap { TAB_LEFT
keymap U NULL
keymap ";"-o GOTO
keymap ";"-t TAB_GOTO
keymap d CLOSE_TAB
keymap T TAB_MENU
#Bookmark operation
#Search
keymap C-s NULL
keymap / ISEARCH
keymap ? ISEARCH_BACK

#Dictionary look-up
#Mark operation
keymap v MARK_WORD
#Miscellany
keymap ";"-s VIEW
keymap ":"-! EXEC_SHELL
keymap M NULL
keymap ESC-M NULL
keymap e-e EXTERN
keymap e-l EXTERN_LINK
keymap c COMMAND
keymap ";"-m MOVE_LIST_MENU
keymap ";"-? HELP
keymap ";"-p OPTIONS
keymap D NULL
keymap ";"-d DOWNLOAD_LIST
keymap C-k NULL
keymap ";"-c COOKIE
keymap ~ REINIT

#User defined keymaps
keymap ";"-b NULL
keymap ";"-b EXTERN "echo %s | xsel --clipboard --input"
keymap V NULL

